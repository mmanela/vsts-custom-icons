{
    "manifest_version": 2,
    "name": "VSTS Custom Icons",
    "description": "Upload your own icons on Visual Studio Team Services",
    "version": "1.0.2",
    "permissions": [
        "activeTab",
        "storage",
        "https://ajax.googleapis.com/"
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://*.visualstudio.com/*"
            ],
            "css": [],
            "js": [
                "dist/customIcons.js"
            ]
        }
    ]
}